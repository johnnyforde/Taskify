<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:layout_width="205dp"
            android:layout_height="70dp"
            android:id="@+id/imageButton9"
            android:src="@drawable/week"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="205dp"
            android:layout_height="70dp"
            android:id="@+id/imageButton8"
            android:src="@drawable/month"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageButton10"
            android:src="@drawable/addevent"
            android:background="#ffff"
            android:layout_below="@+id/imageButton9"
            android:layout_alignEnd="@+id/button7" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="&lt;"
            android:id="@+id/button6"
            android:layout_below="@+id/imageButton10"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="January 2016"
            android:id="@+id/textView36"
            android:layout_alignTop="@+id/button6"
            android:layout_centerHorizontal="true" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=">"
            android:id="@+id/button7"
            android:layout_alignTop="@+id/textView36"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:layout_below="@+id/button6"
            android:layout_alignParentStart="true"
            android:src="@drawable/days" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/imageView"
            android:layout_alignParentStart="true">

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView3"
                tools:listitem="@android:layout/simple_list_item_1" />

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView4" />

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView5" />

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView6" />

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView7" />

            <ListView
                android:layout_width="59dp"
                android:layout_height="400dp"
                android:id="@+id/listView8" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ListView
                    android:layout_width="wrap_content"
                    android:layout_height="170dp"
                    android:id="@+id/listView2"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentStart="true" />

                <ListView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView9"
                    android:layout_below="@+id/listView2"
                    android:layout_alignParentStart="true" />
            </RelativeLayout>
        </LinearLayout>

    </RelativeLayout>
</LinearLayout>
